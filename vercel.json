{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/relay-control",
        "dest": "/relay-control.html"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ]
  }
  